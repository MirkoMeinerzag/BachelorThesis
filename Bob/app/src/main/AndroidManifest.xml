<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.projects.bob">
<application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme"
    tools:ignore="GoogleAppIndexingWarning">
    <!-- ATTACK TWO -->
    <activity
        android:name=".attack2.B2_Attack2"
        android:exported="true"
        android:launchMode="singleTask">
        <intent-filter>
            <action android:name="com.projects.bob.STARTB2ATT2" />
            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
    </activity>
    <!-- ATTACK FIVE -->
    <activity android:name=".attack5.B2_Attack5"
        android:allowTaskReparenting="true"
        android:taskAffinity="com.projects.bob">
        <intent-filter>
            <action android:name="com.projects.bob.STARTB2ATT5" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
    </activity>

</application>

</manifest>